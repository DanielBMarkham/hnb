    <script type="text/javascript">
        var displayIntervalID = 0;
        var defaultEventDate = new Date("12/21/2012");
        var defaultEventName = "Mayan End Of World";
        var modelVariables = new Object();
        var interestingEvent = new Object();
        var monthlyIncomePerUser = 0;
        var monthlyBurnRatePerUser = 0;
        var MONTHCOUNT = 120;
        var monthlyStatus = new Array(MONTHCOUNT);


        var defaultParms = new Object();
            defaultParms.ModelName = "no model data entered";
            defaultParms.StartDate = new Date();
            defaultParms.CashOnHand = 5000;
            defaultParms.InitialMonthlyIncome = 10;
            defaultParms.InitialUserCount = 10000;
            defaultParms.BurnRateFixed = 20;
            defaultParms.BurnRateVariable = 35;
            defaultParms.MonthlyOrganicProspects = 100;
            defaultParms.ProspectToCustomerConversionRate = .02;
            defaultParms.CostToBuyAProspect = 2.5;
            defaultParms.AverageSalesEventsPerUserPerMonth = 1;
            defaultParms.MonthlyCustomerAttritionRate = .05;
            defaultParms.NetworkEffect = 0;
            defaultParms.GrowthRateYoureWillingToPayFor = .01;
            function makeIntoNum(num){
                temp = 0;
                if (num==null){
                    temp = 0;
                }
                else
                {
                    try{
                        temp = parseFloat(num);
                    }
                    catch(e)
                    {
                        temp = 0;
                    }
                }
                return temp;
            }
            function makeIntoStr(item){
                ret = "";
                if (null == item){
                    ret ="";
                }
                else
                {
                    ret=item;
                }
                return ret;
            }
            function makeIntoDate(item){
                if (null != item) {
                    try {
                        return new Date(item);
                    }
                    catch (e) {
                        return new Date();
                    }
                }
                else {
                    return new Date();
                }
            }
            function stuffModelVariablesIntoBoxes() {
                modelNameInput.value =                  makeIntoStr(modelVariables.ModelName);
                $("#modelTitle").html(makeIntoStr(modelVariables.ModelName));
                temp = makeIntoDate(modelVariables.StartDate);
                $("#startDateInput").datepicker();
                $("#startDateInput").datepicker("setDate", temp);
                cashOnHandInput.value =                 makeIntoNum(modelVariables.CashOnHand);
                initialMonthlyIncomeInput.value =       makeIntoNum(modelVariables.InitialMonthlyIncome);
                initialUserCountInput.value =           makeIntoNum(modelVariables.InitialUserCount);
                burnRateFixedInput.value =              makeIntoNum(modelVariables.BurnRateFixed);
                burnRateVariableInput.value =           makeIntoNum(modelVariables.BurnRateVariable);
                monthlyOrganicProspects.value =         makeIntoNum(modelVariables.MonthlyOrganicProspects);
                prospectCustomerConversionRate.value =  makeIntoNum(modelVariables.ProspectToCustomerConversionRate);
                costToBuyAProspect.value =              makeIntoNum(modelVariables.CostToBuyAProspect);
                averageSalesEventsPerUserPerMonth.value = makeIntoNum(modelVariables.AverageSalesEventsPerUserPerMonth);
                monthlyCustomerAttritionRate.value =    makeIntoNum(modelVariables.MonthlyCustomerAttritionRate);
                networkEffectInput.value =              makeIntoNum(modelVariables.NetworkEffect);
                growthRateYoureWillingToPayFor.value =  makeIntoNum(modelVariables.GrowthRateYoureWillingToPayFor);
            }
            function stuffBoxesIntoModelVariables() {
                modelVariables.ModelName = modelNameInput.value;
                modelVariables.StartDate = startDateInput.value;
                modelVariables.CashOnHand = cashOnHandInput.value;
                modelVariables.InitialMonthlyIncome = initialMonthlyIncomeInput.value;
                modelVariables.InitialUserCount = initialUserCountInput.value;
                modelVariables.BurnRateFixed = burnRateFixedInput.value;
                modelVariables.BurnRateVariable = burnRateVariableInput.value;
                modelVariables.MonthlyOrganicProspects = monthlyOrganicProspects.value;
                modelVariables.ProspectToCustomerConversionRate = prospectCustomerConversionRate.value;
                modelVariables.CostToBuyAProspect = costToBuyAProspect.value;
                modelVariables.AverageSalesEventsPerUserPerMonth = averageSalesEventsPerUserPerMonth.value;
                modelVariables.MonthlyCustomerAttritionRate = monthlyCustomerAttritionRate.value;
                modelVariables.NetworkEffect = networkEffectInput.value;
                modelVariables.GrowthRateYoureWillingToPayFor = growthRateYoureWillingToPayFor.value;
                // derived variables
                monthlyBurnRatePerUser = modelVariables.BurnRateVariable / modelVariables.InitialUserCount;
                monthlyIncomePerUser = modelVariables.InitialMonthlyIncome / modelVariables.InitialUserCount;    
            }
            function createStatusItem(month, lastMonthsUserCount, lastMonthsCashBalance) {
                newNetworkedProspects = lastMonthsUserCount * modelVariables.NetworkEffect * 1;
                newOrganicProspects = modelVariables.MonthlyOrganicProspects * 1;
                desiredEndUserCount = parseInt(lastMonthsUserCount * (1 + modelVariables.GrowthRateYoureWillingToPayFor * 1));
                lastMonthsUserCountMinusAttrition = lastMonthsUserCount * (1 - modelVariables.MonthlyCustomerAttritionRate * 1);
                if (modelVariables.GrowthRateYoureWillingToPayFor * 1 == 0) desiredEndUserCount = lastMonthsUserCountMinusAttrition; // if you don't want to pay for any new users, whatever you have is fine
                neededNewUsers = desiredEndUserCount - lastMonthsUserCountMinusAttrition
                neededNewProspects = neededNewUsers /modelVariables.ProspectToCustomerConversionRate;
                newPaidForProspects = neededNewProspects - newOrganicProspects;
                //if (neededNewProspects == 0) newPaidForProspects = 0;
                if (newPaidForProspects <= 0) newPaidForProspects = 0;
                costForNewProspects = 0 + newPaidForProspects * modelVariables.CostToBuyAProspect / modelVariables.ProspectToCustomerConversionRate;
                newProspects = (newNetworkedProspects + newOrganicProspects + newPaidForProspects) * 1;
                newUsersFromProspects = (newProspects * modelVariables.ProspectToCustomerConversionRate * 1);
                newUserCount = parseInt(lastMonthsUserCount * 1 + newUsersFromProspects);
                costOfSupportingUsers = parseInt(100 *newUserCount * monthlyBurnRatePerUser)/100;
                monthlyExpenses = parseInt(((modelVariables.BurnRateFixed*1) + costOfSupportingUsers) * 100) / 100;
                monthlyIncome = parseInt(newUserCount * monthlyIncomePerUser *100)/100;
                newCashOnHand = (parseInt(lastMonthsCashBalance*1 + monthlyIncome*1 - monthlyExpenses*1) * 100)/100;
                ret = new Object();
                ret.MonthBegin = month;
                ret.BeginningBalance = lastMonthsCashBalance;
                ret.BeginningUserCount = lastMonthsUserCount;
                ret.CostOfNewUsers = costForNewProspects;
                ret.VariableExpenses = costOfSupportingUsers;
                ret.FixedExpenses = modelVariables.BurnRateFixed;
                ret.TotalExpenses = monthlyExpenses;
                ret.TotalIncome = monthlyIncome;
                ret.EndingUserCount = newUserCount;
                ret.EndingBalance = newCashOnHand;
                return ret;
            }
            function findInterestingEvent() {
                ret = new Object();
                for (i = 1; i < MONTHCOUNT; i++) {
                if ( (monthlyStatus[i].EndingBalance <0) && (monthlyStatus[i-1].EndingBalance>0)){
                    ret.EventName = "DEATH";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                    }
                if ((monthlyStatus[i].TotalIncome > 1000) && (monthlyStatus[i-1].TotalIncome < 1000)) {
                    ret.EventName = "$1K Total Income Per Month";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].EndingUserCount > 1000) && (monthlyStatus[i-1].EndingUserCount < 1000)) {
                    ret.EventName = "1K Monthly Users";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].TotalIncome > 5000) && (monthlyStatus[i - 1].TotalIncome < 5000)) {
                    ret.EventName = "$5K Total Income Per Month";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].EndingUserCount > 5000) && (monthlyStatus[i - 1].EndingUserCount < 5000)) {
                    ret.EventName = "5K Monthly Users";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].TotalIncome > 10000) && (monthlyStatus[i - 1].TotalIncome < 10000)) {
                    ret.EventName = "$10K Total Income Per Month";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].EndingUserCount > 10000) && (monthlyStatus[i - 1].EndingUserCount < 10000)) {
                    ret.EventName = "10K Monthly Users";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].TotalIncome > 50000) && (monthlyStatus[i - 1].TotalIncome < 50000)) {
                    ret.EventName = "$50K Total Income Per Month";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].EndingUserCount > 50000) && (monthlyStatus[i - 1].EndingUserCount < 50000)) {
                    ret.EventName = "50K Monthly Users";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].TotalIncome > 100000) && (monthlyStatus[i - 1].TotalIncome < 100000)) {
                    ret.EventName = "$100K Total Income Per Month";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].EndingUserCount > 100000) && (monthlyStatus[i - 1].EndingUserCount < 100000)) {
                    ret.EventName = "100K Monthly Users";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].TotalIncome > 1000000) && (monthlyStatus[i - 1].TotalIncome < 1000000)) {
                    ret.EventName = "$1M Total Income Per Month";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].EndingUserCount > 1000000) && (monthlyStatus[i - 1].EndingUserCount < 1000000)) {
                    ret.EventName = "1M Monthly Users";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].TotalIncome > 10000000) && (monthlyStatus[i - 1].TotalIncome < 10000000)) {
                    ret.EventName = "$10M Total Income Per Month";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].EndingUserCount > 10000000) && (monthlyStatus[i - 1].EndingUserCount < 10000000)) {
                    ret.EventName = "10M Monthly Users";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].TotalIncome > 100000000) && (monthlyStatus[i - 1].TotalIncome < 100000000)) {
                    ret.EventName = "$100M Total Income Per Month";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].EndingUserCount > 100000000) && (monthlyStatus[i - 1].EndingUserCount < 100000000)) {
                    ret.EventName = "100M Monthly Users";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].TotalIncome > 500000000) && (monthlyStatus[i - 1].TotalIncome < 500000000)) {
                    ret.EventName = "$500M Total Income Per Month";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].EndingUserCount > 500000000) && (monthlyStatus[i - 1].EndingUserCount < 500000000)) {
                    ret.EventName = "500M Monthly Users";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].TotalIncome > 1000000000) && (monthlyStatus[i - 1].TotalIncome < 1000000000)) {
                    ret.EventName = "$1B Total Income Per Month";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
                if ((monthlyStatus[i].EndingUserCount > 1000000000) && (monthlyStatus[i - 1].EndingUserCount < 1000000000)) {
                    ret.EventName = "1B Monthly Users";
                    ret.EventDate = new Date(modelVariables.StartDate).add(i).months();
                    break;
                }
            }
            if (null == ret.EventName) {
                ret.EventName = "-" + defaultEventName + "-";
                ret.EventDate = defaultEventDate;
            }
            return ret;
            }
            function getMax(){
                var temp = 0;
                for (i = 0; i < MONTHCOUNT; i++) {
                    if (Math.abs(monthlyStatus[i].EndingBalance) > temp) temp = Math.abs(monthlyStatus[i].EndingBalance);
                }
                return temp;
            }
            function getInc(){
                var temp = new Array(MONTHCOUNT);
                for (i = 0; i < MONTHCOUNT; i++) {
                    temp.push([i,monthlyStatus[i].TotalIncome]);
                }
                return temp;
            }
            function getExp(){
                var temp = new Array(MONTHCOUNT);
                for (i = 0; i < MONTHCOUNT; i++) {
                    temp.push([i,monthlyStatus[i].TotalExpenses]);
                }
                return temp;
            }
            function getBal(){
                var temp = new Array(MONTHCOUNT);
                for (i = 0; i < MONTHCOUNT; i++) {
                    temp.push([i,monthlyStatus[i].EndingBalance]);
                }
                return temp;
            }
            function fillArray() {
                $("#modelTable").html("");
                $("#modelTable").append("<thead><tr><td>Month</td><td>Beginning Balance</td><td>Beginning Users</td><td>Income</td><td>Expenses</td><td>Ending Users</td><td>Ending Balance</td></tr><thead>");
                loopMonth = new Date(modelVariables.StartDate);
                monthlyStatus[0] = createStatusItem(loopMonth, modelVariables.InitialUserCount, modelVariables.CashOnHand);
                $("#modelTable").append("<thead><tr><td>" + loopMonth.toString("MMM") + "-" + loopMonth.toString("yyyy") + "</td><td>" + monthlyStatus[0].BeginningBalance + "</td><td>" + monthlyStatus[0].BeginningUserCount + "</td><td>" + monthlyStatus[0].TotalIncome + "</td><td>" + monthlyStatus[0].TotalExpenses + "</td><td>" + monthlyStatus[0].EndingUserCount + "</td><td>" + monthlyStatus[0].EndingBalance + "</td></tr><thead>");
                for (i = 1; i < MONTHCOUNT; i++) {
                    loopMonth = loopMonth.add(1).month();
                    monthlyStatus[i] = createStatusItem(loopMonth, monthlyStatus[i - 1].EndingUserCount, monthlyStatus[i - 1].EndingBalance);
                    $("#modelTable").append("<tr><td>" + loopMonth.toString("MMM") + "-" + loopMonth.toString("yyyy") + "</td><td>" + monthlyStatus[i].BeginningBalance + "</td><td>" + monthlyStatus[i].BeginningUserCount + "</td><td>" + monthlyStatus[i].TotalIncome + "</td><td>" + monthlyStatus[i].TotalExpenses + "</td><td>" + monthlyStatus[i].EndingUserCount + "</td><td>" + monthlyStatus[i].EndingBalance + "</td></tr>");
                }
                ievt = findInterestingEvent();
                interestingEvent = ievt;
                iMax = getMax();
                inco = getInc();
                expe = getExp();
                bala = getBal();
                try{
                    $.plot($("#graphPlaceholder"), [inco, expe, bala], { yaxis: { max: iMax, min: -1*iMax } });
                }
                catch(e){
                }
            }
            function boxesHaveChanged(sender) {
                stuffBoxesIntoModelVariables();
                $.bbq.pushState(modelVariables);
                fillArray();
            }
            function updateDisplay(sender) {
                eventName = defaultEventName;
                if (interestingEvent.EventName != null) eventName = interestingEvent.EventName;
                eventDate = defaultEventDate;
                if (interestingEvent.EventDate != null) eventDate = interestingEvent.EventDate;

            var now = new Date();
            var current = parseInt(now.getTime()/1000);
            var diff_secs = eventDate.getTime()/1000 - current;

            diff_days = parseInt(diff_secs / (60 * 60 * 24));
            d2 = diff_secs - (diff_days * 60 * 60 * 24);
            diff_hours = parseInt(d2 / (60 * 60));
            d3 = d2 - (diff_hours * 60 * 60);
            diff_minutes = parseInt(d3 / (60));
            d4 = d3 - (diff_minutes * 60);
            
            $("#display").html(eventName + "<br/><br/>" + diff_days + " Days " + diff_hours + " Hrs " + diff_minutes + " Min " + d4 + " secs");
            //alert(cashOnHandInput.value);
        }
        function loadModelVariablesFromHash(){
            // set up defaults (or load querystring inputs)
            v = $.bbq.getState();
            if ( (null == v) || (null == v.ModelName) ) {
                modelVariables = defaultParms;
            }
            else {
                modelVariables = v;
                // derived variables
                monthlyBurnRatePerUser = modelVariables.BurnRateVariable / modelVariables.InitialUserCount;
                monthlyIncomePerUser = modelVariables.InitialMonthlyIncome / modelVariables.InitialUserCount;
                fillArray();
                findInterestingEvent();
            }
        }
        $(document).ready(function () {
            loadModelVariablesFromHash();
            stuffModelVariablesIntoBoxes();
            displayIntervalID = window.setInterval(updateDisplay, 500);
            $("#enterModelData").effect("pulsate", {}, 750, null);
        });
        $(function () {
            $("#startDateInput").datepicker();
            $(window).bind('hashchange', function (e) {
                loadModelVariablesFromHash();
                stuffModelVariablesIntoBoxes();
            });
        });
        </script>
</head>
